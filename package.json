  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "resolutions": {
    "styled-components": "^5"
