    "expo-font": "~11.0.1",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-web": "~0.18.9",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/styled-components": "^5.1.26",
